-- @nsURI MM=uri:http://www.eclipse.org/uml2/5.0.0/UML
-- @atlcompiler atl2010

module Move;
create OUT : MM refining IN : MM;


rule CopieModel {
	from
		model : MM!Model,enum : MM!Enumeration
	to 
		m2 : MM!Model  (
			name <- 'ModeleSortie',
			packageImport <- model.packageImport,
			packagedElement <- model.packagedElement.including(enumAdd)
		),
		enumAdd : MM!Enumeration (
			name <- enum.name,
			ownedLiteral <- enum.ownedLiteral
		)
}
